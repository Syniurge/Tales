execute_process(COMMAND @DIFF_EXECUTABLE@ -u
		"@CMAKE_CURRENT_SOURCE_DIR@/@MODFILE_ORIG@"
		"@CMAKE_CURRENT_SOURCE_DIR@/@MODFILE@"
	OUTPUT_FILE "@CMAKE_CURRENT_BINARY_DIR@/patches/@MODFILE_BASENAME@.patch"
	RESULT_VARIABLE DIFF_RESULT
)

if("${DIFF_RESULT}" GREATER "1")
  message(FATAL_ERROR "@MODFILE_BASENAME@.patch generation failed!")
endif()
